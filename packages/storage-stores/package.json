{
	"name": "@elsa-health/storage-stores",
	"version": "0.1.0",
	"description": "Stores to be used with `@elsa-health/storage` library",
	"private": false,
	"sideEffects": false,
	"files": [
		"local/**",
		"remote/**"
	],
	"scripts": {
		"test": "jest -u",
		"build": "tsc -p .",
		"dev": "tsup ./src --format cjs --watch --dts --external \"@sabertooth/storage\" --external \"react-native-mmkv\" --out-dir ./",
		"lint": "TIMING=1 eslint src --fix",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf ./local ./remote"
	},
	"keywords": [
		"storage",
		"local",
		"remote"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"author": "",
	"license": "Apache-2.0",
	"peerDependencies": {
		"@elsa-health/storage": "*",
		"react-native-mmkv": "*"
	},
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/mongodb": "^4.0.7",
		"@types/node": "^17.0.21",
		"jest": "^27.5.1",
		"react-native-mmkv": "^2.2.0",
		"tsup": "^5.12.1",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"axios": "^0.26.1",
		"query-string": "^7.1.1",
		"rxjs": "^7.5.5"
	}
}
