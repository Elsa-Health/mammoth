{
  "name": "@elsa-ui/react-native",
  "version": "0.1.0",
  "description": "Elsa.Health React Native UI Library",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "components/",
    "theme/",
    "visuals/",
    "README.md",
    "!**/__tests__/**",
    "!**/__mocks__/**",
    "!**/__stories__/**",
    "!.circleci",
    "!.husky",
    "!example"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "build-types": "tsc -p .",
    "build-babel": "babel ./src --out-dir ./ --source-maps --extensions .ts,.tsx --ignore \"src/**/*.test.tsx\",\"src/**/*.test.js\",\"tsconfig.json\",\"tsconfig.build.json\"",
    "build": "yarn build-types && yarn build-babel",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "clean": "rm -rf node_modules && yarn cache clean && rm -rf ./components ./theme ./visuals tsconfig.tsbuildinfo",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/elsa-health/react-native-elsa-ui",
  "author": "iam-kevin <kevin@elsa.health> (https://github.com/iam-kevin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elsa-health/react-native-elsa-ui/issues"
  },
  "homepage": "https://github.com/elsa-health/react-native-elsa-ui#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/config-conventional": "^11.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@testing-library/react-native": "^9.1.0",
    "@types/react": "^17.0.31",
    "@types/react-native": "^0.66.0",
    "@types/styled-components": "^5.1.24",
    "@types/styled-components-react-native": "^5.1.3",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react-native-paper": "^4.12.0",
    "react-native-svg": "^12.3.0",
    "release-it": "^14.2.2",
    "styled-components": "^5.3.5",
    "typescript": "^4.4.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-paper": "*",
    "react-native-svg": "*",
    "styled-components": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "./lib",
    "targets": [
      "commonjs",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@react-native-picker/picker": "^2.4.0",
    "immer": "^9.0.12",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-sectioned-multi-select": "^0.9.1"
  }
}
