{
	"name": "@elsa-ui/workflows",
	"version": "1.0.0",
	"main": "./index.js",
	"sideEffects": false,
	"license": "MIT",
	"files": [
		"screens/**",
		"screen-group/**",
		"flows/**"
	],
	"scripts": {
		"postinstall": "rm -rf node_modules/react-native",
		"test": "jest -u",
		"build": "tsup ./src --format cjs --dts --external react-native  --out-dir ./",
		"dev": "tsup ./src --format cjs --watch --dts --external react-native --out-dir ./",
