{
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**"]
		},
		"test": {
			"dependsOn": ["^test"]
		},
		"coverage": {
			"dependsOn": ["^coverage"],
			"outputs": ["coverage/**"]
		},
		"coverage:ci": {
			"dependsOn": ["^coverage:ci"],
			"outputs": ["coverage/**"]
		},
		"lint": {
			"outputs": []
		},
		"clean": {
			"cache": false
		},
		"dev": {
			"cache": false
		}
	},
	"globalDependencies": ["$GITHUB_TOKEN"]
}
